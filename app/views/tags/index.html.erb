<h1> All of the Tags</h1>

<ul id="tags">
  <% @tags.each do |tag| %>
    <li>
      <%= link_to tag.name, tag_path(tag), class: 'tag_name', name: "tag_#{tag.id}" %>
    </li>
  <% end %>
</ul>









